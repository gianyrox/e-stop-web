{
  "manifest_version": 3,
  "name": "estop",
  "description": "Share links with anyone",
  "version": "1.0",
  "icons": {
    "16": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  },
  "action": {
    "default_icon": {
      "16": "/images/logo.png",
      "48": "/images/logo.png",
      "128": "/images/logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "css": ["extension.css"]
    }
  ],
  "permissions": ["tabs"]
}
